#!/usr/bin/env python3

class Stack(object):
   def __init__(self):
     self.s = []
   def push(self, item):
     return self.s.append(item)
   def pop(self):
     return self.s.pop()
   def top(self):
     return self.s[-1]
   def is_empty(self):
     return len(self.s) == 0
   def __len__(self):
     return len(self.s)

